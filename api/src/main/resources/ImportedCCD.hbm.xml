<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.exportccd">


	<class name="ImportedCCD"
		table="imported_ccd" >
		<id name="id" type="int" column="id" unsaved-value="0">
			<generator class="native" />
		</id>
	
		<many-to-one name="importedFor" class="org.openmrs.Patient"
			not-null="true" column="patient_id" />
		<many-to-one name="importedBy" class="org.openmrs.User"
			not-null="false" column="imported_by" />
		<property name="dateImported" type="java.util.Date" column="date_imported" />
		<property name="ccdImported" type="java.lang.String" column="ccd" />
	
	</class>
	
</hibernate-mapping>
